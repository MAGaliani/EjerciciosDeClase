<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.magaliani.galoweather.TiempoActualFragment">

    <ImageView
        android:id="@+id/ImageViewfondo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_launcher_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageViewSun"
        android:layout_width="123dp"
        android:layout_height="97dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewCity"
        app:srcCompat="@drawable/ic_sunny" />

    <ImageView
        android:id="@+id/imageViewThermometer"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewSun"
        app:srcCompat="@drawable/ic_thermometer" />

    <ImageView
        android:id="@+id/imageViewHumidity"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewThermometer"
        app:srcCompat="@drawable/ic_humidity" />

    <ImageView
        android:id="@+id/imageViewWind"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewHumidity"
        app:layout_constraintVertical_bias="0.046"
        app:srcCompat="@drawable/ic_wind" />

    <TextView
        android:id="@+id/textViewTemp"
        android:layout_width="129dp"
        android:layout_height="66dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="Temperatura en ºC"
        android:textAlignment="textEnd"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageViewThermometer"
        app:layout_constraintTop_toTopOf="@+id/imageViewThermometer" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="100dp"
        android:layout_height="66dp"
        android:layout_marginEnd="50dp"
        android:layout_marginStart="1dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="ºC"
        android:textAlignment="textStart"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textViewTemp"
        app:layout_constraintTop_toTopOf="@+id/textViewTemp" />

    <TextView
        android:id="@+id/textViewHumedad"
        android:layout_width="129dp"
        android:layout_height="66dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="Humedad en %"
        android:textAlignment="textEnd"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageViewHumidity"
        app:layout_constraintTop_toBottomOf="@+id/textViewTemp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="100dp"
        android:layout_height="66dp"
        android:layout_marginEnd="50dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="% De humedad"
        android:textAlignment="center"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toEndOf="@+id/textViewTemp"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/TextViewViento"
        android:layout_width="129dp"
        android:layout_height="66dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="29dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="Viento en KPH"
        android:textAlignment="textEnd"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageViewWind"
        app:layout_constraintTop_toBottomOf="@+id/textViewHumedad" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="100dp"
        android:layout_height="66dp"
        android:layout_marginEnd="50dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="29dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="Millas/Hora"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textViewTemp"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textViewCity"
        android:layout_width="367dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/alfa_slab_one"
        android:gravity="center"
        android:text="Ciudad"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/autoCompleteTextView" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView"
        android:layout_width="319dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:hint="Buscar..."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp" />

</android.support.constraint.ConstraintLayout>
